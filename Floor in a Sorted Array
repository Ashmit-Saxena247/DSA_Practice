class Solution:
    def findFloor(self, arr, x):
        l = 0
        r = len(arr)-1
        floor = -1
        
        while l <= r:
            mid = (l+r)//2
            
            if arr[mid] == x:
                floor = mid;
                l = mid +1
            elif arr[mid] < x :
                floor = mid
                l = mid + 1
            else:
                r = mid - 1
            
        return floor

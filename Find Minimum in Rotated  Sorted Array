class Solution(object):
    def findMin(self, nums):
        l, r = 0, len(nums) - 1
        result = nums[0]

        while(l<=r):
            if(nums[l]<nums[r]):
                result = min(result,nums[l])
            mid = (l+r)//2
            result = min(result,nums[mid])

            if nums[l] < nums[mid]:
                l = mid + 1
            else:
                r = mid -1
        return result

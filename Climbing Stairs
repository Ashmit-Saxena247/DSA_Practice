class Solution(object):
    def climbStairs(self, n): 
        if n <= 2:
            return n
        dp=[0]*(n + 1)
        dp[1] =1
        dp[2] =2
        for i in range(3,n + 1):
            dp[i] = dp[i - 1]+dp[i - 2]

        return dp[n] 

===============================================================================================================================================================================================================

class Solution(object):
    def climbStairs(self, n): 
        if(n<2):
            return n
        a , b = 1 , 1

        for i in range(n-1):
            a , b = a+b , a
        return a
